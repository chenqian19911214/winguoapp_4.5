        android:paddingTop="@dimen/click_login_pt"
        android:textColor="@color/wh"
        android:lineSpacingExtra="2dp"
        android:lineSpacingMultiplier="1"
        android:layout_gravity="center"
        android:layout_marginBottom="60dp"/>

</FrameLayout>